<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/fragmentMenuLarge_id"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/board"
    >
    <LinearLayout
        android:id="@+id/fragmentMenuLarge_row1"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="horizontal"
        >
        <LinearLayout
            android:id="@+id/fragmentMenuLarge_panel1"
            android:layout_margin="2dp"
            android:layout_height="match_parent"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:orientation="vertical"
            android:background="@drawable/keypad_button"
            android:gravity="center"
            >
            <TextView
                android:id="@+id/fragmentMenuLarge_label1"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:layout_margin="1dp"
                android:textColor="@color/text_light"
                android:textSize="@dimen/textsize_normal"
                android:minLines="2"
                android:gravity="center"
                />
            <ImageButton
                android:id="@+id/fragmentMenuLarge_button1"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:layout_margin="1dp"
                android:scaleType="fitCenter"
                android:adjustViewBounds="true"
                android:clickable="false"
                />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/fragmentMenuLarge_panel3"
            android:layout_margin="2dp"
            android:layout_height="match_parent"
            android:layout_width="0dp"
            android:layout_weight="1"
